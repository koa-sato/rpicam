<% if notice %>
<p id="notice"><%= notice %></p>
<% end %>

<h1>Log in</h1>

<table>
  <thead>
    <tr>
      <th>Users</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.username %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_user_path %>
<%= link_to 'Log in', camera_path %>